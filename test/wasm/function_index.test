# RUN: yaml2obj < %p/Inputs/ret32.yaml > %T/ret32.o
# RUN: yaml2obj < %p/Inputs/ret64.yaml > ret64.o
# RUN: lld -flavor wasm -r -o %t.wasm %T/ret32.o ret64.o
# RUN: obj2yaml %t.wasm | FileCheck %s

CHECK: Sections:
CHECK:   - Type:            TYPE
CHECK:     Signatures:
CHECK:       - Index:           0
CHECK:         ReturnType:      I32
CHECK:         ParamTypes:
CHECK:           - F32
CHECK:       - Index:           1
CHECK:         ReturnType:      I64
CHECK:         ParamTypes:
CHECK:           - F64
CHECK: - Type:            FUNCTION
CHECK:   FunctionTypes: [ 0, 1 ]
